.container#prices.scrollspy
    section.prices
        .row
            .col.s12
                h2 Pricing
        .row
            .col.s12.l4.margin-on-medium-and-down
                .price-box
                    h4 Beginner
                    h5
                        sup $
                        | 19
                    .divider
                    ul.flow-text
                        li 5 users
